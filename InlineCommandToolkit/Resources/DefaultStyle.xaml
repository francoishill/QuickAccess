<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
										xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary>
			<LinearGradientBrush x:Key='WindowBackgroundBrush'
													 StartPoint="0,0"
													 EndPoint="0,1">
				<GradientStop Color="#fff"
											Offset="0" />
				<GradientStop Color="#eee"
											Offset="0.7" />
				<GradientStop Color="#f0f0f0"
											Offset="1" />
			</LinearGradientBrush>
			<SolidColorBrush x:Key='CommandArgumentAutoCompleteBoxBackgroundBrush'
											 Color='#fff' />
			<LinearGradientBrush x:Key='CommandArgumentAutoCompleteBoxDropdownItemBackground'
													 StartPoint='0,0'
													 EndPoint='0,1'>
				<GradientStop Color='#ddd'
											Offset='0' />
				<GradientStop Color='#f0f0f0'
											Offset='0.75' />
				<GradientStop Color='#ddd'
											Offset='1' />
			</LinearGradientBrush>
			<SolidColorBrush x:Key='CommandArgumentAutoCompleteBoxDropdownItemForeground'
											 Color='#333' />
		</ResourceDictionary>
	</ResourceDictionary.MergedDictionaries>
</ResourceDictionary>